        <!-- Pdf viewer -->
        <content>
            <PDFViewer source="/assets/pdf.pdf" width="50%" height="50%" />
        </content>

        <!-- Invoice List Filter -->
        <content>
            <List 
                items="{datas>/books}"
                id="invoiceList"
            >
                <headerToolbar>
                    <Toolbar >
                        <Title text="Invoice List"></Title>
                        <ToolbarSpacer />
                        <SearchField search=".onInvSearch" width="50%" ></SearchField>
                    </Toolbar>
                </headerToolbar>
                <items>
                    <ObjectListItem 
                        title="{datas>title}" 
                        number="{datas>quantity}"
                        numberState="{=${datas>quantity} > 15 ? 'Success' : 'Error'}"
                        numberUnit="EUR"
                    >
                        <firstStatus>
                            <ObjectStatus 
                                text="{
                                    path:'datas>Status',
                                    formatter:'.formatter.statusText'
                                }"
                            ></ObjectStatus>
                        </firstStatus>
                    </ObjectListItem>
                </items>
            </List>
        </content>

        <!-- Navigation -->
        <content>
            <Title text="Navigation" width="100%"></Title>
            <GenericTile header="Manage Activity of users" subheader="Subtitle" press="openNav">
                <TileContent unit="EUR" footer="Current Quarter">
                    <ImageContent src="sap-icon://home-share" />
                </TileContent>
            </GenericTile>
        </content>

        <!-- Split App Tile -->
        <content>
            <Title text="Split App" width="100%"/>
            <GenericTile header="Split App" press="openSplitApp" >
                <TileContent footer="App"/>
            </GenericTile>
        </content>

        <!-- check box -->
        <content>
            <Title text="check box" width="100%"/>
            <l:VerticalLayout >
                <Text text="Which language do u speek ?"></Text>
                <CheckBox 
                    text="Select All / Deselect All"
                    selected="{= ${/child1} || ${/child2} || ${/child3}}"
                    partiallySelected="{= !(${/child1} &amp;&amp; ${/child2} &amp;&amp; ${/child3})}"
                    select="onParentClicked" 
                ></CheckBox>
                <CheckBox text="English" selected="{/child1}"/>
                <CheckBox text="German" selected="{/child2}" />
                <CheckBox text="French" selected="{/child3}"/>
            </l:VerticalLayout>
        </content>
    </Page>
</mvc:View>