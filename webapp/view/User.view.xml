<mvc:View
    xmlns:mvc="sap.ui.core.mvc" 
    xmlns="sap.m" 
    displayBlock="true"
    controllerName="ns.buinessparter.controller.User" 
>
    <Page id="userpage">
        <content>
            <Title text="{user>/userName}" id="uname" width="100%"></Title>
            <Title text="{user>/contactNo}" width="100%" ></Title>
            <Title text="{user>/address}" width="100%" ></Title>
            <Title text="{user>/company}" width="100%" ></Title>
            <Title text="{user>/activity}" width="100%" ></Title>
            <Button text="back" press="back" />
        </content>
    </Page>
</mvc:View>